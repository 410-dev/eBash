#!/bin/bash
@import Foundation/string
@import Foundation/out

function Files.readStringData() {
	if [[ $(isNull "$1") ]]; then
		println "Syntax error in: Files.readStringData - Parameter is not properly given."
		exit
	else
		if [[ ! $(Files.isFile "$2") ]]; then
			println "Runtime error in: Files.readStringData - File does not exist."
			exit
		else
			cat "$1"
		fi
	fi
}

export -f Files.readStringData